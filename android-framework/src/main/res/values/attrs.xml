<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- Font attribute, i.e. which font family -->
	<declare-styleable name="TextMaterialFont">
		<attr name="materialFontFamily" format="enum">
			<enum name="light" value="0"/>
			<enum name="light_italic" value="1"/>
			<enum name="light_bold" value="2"/>
			<enum name="light_bold_italic" value="3"/>
			<enum name="regular" value="4"/>
			<enum name="regular_italic" value="5"/>
			<enum name="regular_bold" value="6"/>
			<enum name="regular_bold_italic" value="7"/>
			<enum name="medium" value="8"/>
			<enum name="condensed" value="9"/>
			<enum name="condensed_italic" value="10"/>
			<enum name="condensed_bold" value="11"/>
			<enum name="condensed_bold_italic" value="12"/>
		</attr>
	</declare-styleable>
</resources>